# Server
PORT=4000
NODE_ENV=development
LOG_LEVEL=info

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/smm?schema=public

# Auth
JWT_SECRET=YOUR_JWT_SECRET
JWT_EXPIRES_IN=7d

# Security
RATE_LIMIT_MAX=120
RATE_LIMIT_WINDOW=1 minute
CORS_ORIGIN=*

# Providers (example)
LEGACY_PROVIDER_URL=https://legacy-provider.example.com
LEGACY_PROVIDER_KEY=changeme

MODERN_PROVIDER_URL=https://modern-provider.example.com/api
MODERN_PROVIDER_KEY=changeme

# Webhooks
WEBHOOK_SIGNING_SECRET=change_this
